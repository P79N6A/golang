!function(){        var eventCenter = require('common:static/ui/eventcenter/eventcenter.js');
        var myplayer = require('common:widget/video/floatPlay/playContainer/playContainer.js');
        myplayer.init(__float_VideoPlayer);
        
        eventCenter.attach('bdv-float-play.play', function (res) {
            // 移除自动播放标识
            $.cookie.set('autoloop', '', {expires: -1});
            myplayer.doPlay(res);
        });

        eventCenter.attach('bdv-float-play.update', function (data) {
            myplayer.doUpdate(data);
        });

        eventCenter.attach('bdv-float-play.close', function () {
            myplayer.doClose();
        });
    }();
!function(){        var eventCenter = require('common:static/ui/eventcenter/eventcenter.js');
        var PlayList = require('common:widget/video/floatPlay/playList/playList.js');
        var plist;
        
        eventCenter.attach('bdv-float-play.list', function (data) {
            bdvFloatList(data);
        });
        
        // 自动播放下一个
        eventCenter.attach('global.playEnd', function () {
            if (plist) {
                // 标记自动播放
                $.cookie.set('autoloop', 1);
                plist.goNext();
            }
        });
        
        // 手动播放下一个
        eventCenter.attach('bdv-float-play.next', function () {
            if (plist) {
                // 移除自动播放标识
                $.cookie.set('autoloop', '', {expires: -1});

                plist.goNext();
            }
        });

        eventCenter.attach('bdv-float-play.close', function () {
            $('#videoListRoot').html('<p class="loading">加载中...</p>');
        });

        function bdvFloatList (data) {
            if (plist) {
                plist.doInit(data);
            }
            else {
                plist = new PlayList({
                    'el': $('#videoListWrap').eq(0),
                    'data': data
                });
            }
        }
    }();
